<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\js\overpassAPIEdit.js" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\js\overpassAPIEdit.js" />
